{
  "name": "mimic-token-cli",
  "version": "1.0.0",
  "description": "CLI tool for managing design tokens with Penpot and Style Dictionary integration",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "mimic-tokens": "dist/cli.js"
  },
  "files": [
    "dist",
    "templates",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && chmod +x dist/cli.js",
    "dev": "tsx src/cli.ts",
    "test": "vitest",
    "start": "node dist/cli.js",
    "prepublishOnly": "pnpm build",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "design-tokens",
    "penpot",
    "style-dictionary",
    "design-system",
    "w3c-dtcg",
    "cli",
    "automation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/IAmJonoBo/mimic-token-cli.git"
  },
  "bugs": {
    "url": "https://github.com/IAmJonoBo/mimic-token-cli/issues"
  },
  "homepage": "https://github.com/IAmJonoBo/mimic-token-cli#readme",
  "author": "Mimic Team",
  "dependencies": {
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "inquirer": "^9.2.12",
    "fs-extra": "^11.2.0",
    "chokidar": "^3.5.3",
    "style-dictionary": "^4.0.0",
    "dotenv": "^16.3.1",
    "axios": "^1.6.0",
    "glob": "^10.3.10",
    "ajv": "^8.12.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "MIT"
}
